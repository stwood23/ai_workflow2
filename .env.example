# Clerk
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_...
CLERK_SECRET_KEY=sk_test_...

# Set the sign in/up URLs if you have custom pages, otherwise Clerk's default pages will be used.
# NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
# NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up

# Set the URLs users are redirected to after signing in or signing up.
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/prompts
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/prompts

# Supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Database connection string (used by Drizzle)
DATABASE_URL=postgresql://user:password@host:port/database

# PostHog
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
NEXT_PUBLIC_STRIPE_PAYMENT_LINK_MONTHLY=
NEXT_PUBLIC_STRIPE_PAYMENT_LINK_YEARLY=

# LLM Providers (add keys for the ones you use)
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
# GROK_API_KEY= # Or other relevant key name
